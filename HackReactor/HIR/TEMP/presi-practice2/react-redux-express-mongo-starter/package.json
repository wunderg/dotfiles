{
  "name": "REMS-Starter",
  "version": "1.0.0",
  "description": "Starter Kit",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development babel-node ./server/server.js",
    "start-prod": "node ./dist/server.js",
    "build-client": "NODE_ENV=production webpack --config ./webpack.production.config.js --profile",
    "build-server": "babel -d ./dist ./server -s",
    "lint": "eslint src; exit 0",
    "test": "mocha --compilers js:babel-core/register --require ignore-styles --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "clean": "rimraf dist",
    "heroku-postbuild": "npm run clean && npm run build-client && npm run build-server"
  },
  "engines": {
    "node": "5.11.0",
    "npm": "3.8.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wunderg/react-redux-express-mongo-starter.git"
  },
  "author": "Oleg Umarov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wunderg/react-redux-express-mongo-starter/issues"
  },
  "homepage": "https://github.com/wunderg/react-redux-express-mongo-starter#readme",
  "dependencies": {
